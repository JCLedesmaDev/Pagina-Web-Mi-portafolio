            "type": "node"
        },
        {
            "preLaunchTask": "start dbm server",
            "type": "chrome",
            "request": "launch",
            "name": "Start debug DBM PLUS",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/dbmPlus/",
            /*"breakOnLoad": true,*/
            /* "sourceMapPathOverrides": {
                "webpack://dbmplus/./src/*": "${webRoot}/*"
            }*/
        },
        {
